    },
    "callExperian":{
        "identityOverPhone":"$('//a[contains(text(),\"I have verified my identity over the phone\")]')",
        "experianUnresolved":"$('//div[contains(text(),\"Experian is still reporting this issue as unresolved\")]')",
        "ridpCode":"$('//span[@id=\"ridp-code\"]')"

    },

    "adminManualVerification":{
        "txt_email":"$('#householdEmail')",
        "btn_Go":"$('//button[contains(text(),\"Go\")]')",
        "lnk_Name":"$$('//td/a')[1]",
        "basicInformationHeader":"$('//h4[contains(text(),\"Basic Information\")]')",
        "btn_remoteIdentityVerificationStatus":"$('#manual_verification')"
    },
    "ajaxLoader":{
        "loader":"$('#loading-spinner')",
        "rightPanel":"$('//div[@id=\"rightpanel\"]')"
    }


    



