        "tb_lastName":"$('[name=\"personSurName\"]')",
        "sb_suffix":"$('[name=\"personNameSuffixText\"]')",
        "tb_dateOfBirth":"$('[name=\"personBirthDate\"]')",
        "tb_ssn":"$('[name=\"personSSNIdentification\"]')",
        "tb_streetAddress":"$('[name=\"address1\"]')",
        "tb_city":"$('[name=\"locationCityName\"]')",
        "sb_state":"$('[name=\"locationStateUSPostalServiceCode\"]')",
        "tb_zipCode":"$('[name=\"locationPostalCode\"]')",
        "tb_primaryPhoneNumber":"$('[name=\"fullTelephoneNumber\"]')",
        "btn_continue":"$('[value=\"CONTINUE\"]')"

    },
    "finish":{
        "errorText":"eval($('//*[@id=\"rightpanel\"]')).$('p')" ,        
