{
  "name": "webdriverio_cucumber",
  "version": "1.0.0",
  "description": "Webdriver with cucumber framework",
  "main": "index.js",
  "scripts": {
    "tt": "./node_modules/.bin/wdio wdio.conf.js",
    "test:lcl": "./node_modules/.bin/cross-env DOCKER=false HEADLESS=false ./node_modules/.bin/wdio wdio.test.conf.js",
    "test:local": "./node_modules/.bin/cross-env DOCKER=false HEADLESS=false ./node_modules/.bin/wdio wdio.conf.js",
    "test:local:headless": "./node_modules/.bin/cross-env DOCKER=false HEADLESS=true ./node_modules/.bin/wdio wdio.conf.js",
    "test:docker": "./node_modules/.bin/cross-env DOCKER=true HEADLESS=true ./node_modules/.bin/wdio wdio.conf.js",
    "test:docker:vnc": "./node_modules/.bin/cross-env DOCKER=true HEADLESS=false ./node_modules/.bin/wdio wdio.conf.js",
    "docker:up": "docker-compose up -d",
    "docker:up:vnc": "docker-compose -f docker-compose.vnc.yml up -d",
    "docker:down": "docker-compose down",
    "docker:down:vnc": "docker-compose -f docker-compose.vnc.yml down",
    "allure-generate": "./node_modules/.bin/allure generate ./allure-results --clean",
    "open-allure": "npm run allure-generate && allure open"
  },
  "author": "Sabita",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@wdio/allure-reporter": "^6.8.1",
    "@wdio/cli": "^6.10.5",
    "@wdio/cucumber-framework": "^6.10.4",
    "@wdio/local-runner": "^6.10.5",
    "@wdio/selenium-standalone-service": "^6.10.4",
    "@wdio/spec-reporter": "^6.8.1",
    "chai": "^4.2.0",
    "chai-webdriverio": "^1.0.0",
    "cross-env": "^7.0.2",
    "cucumber": "^6.0.5",
    "dotenv": "^8.2.0",
    "install": "^0.13.0",
    "npm": "^6.14.9",
    "properties-reader": "^2.0.0",
    "ts-node": "^8.10.1",
    "wdio-chromedriver-service": "^6.0.2",
    "wdio-image-comparison-service": "^2.0.0",
    "webdriverio": "^6.10.5"
  },
  "dependencies": {
    "@types/node": "^14.14.10",
    "@wdio/sync": "^6.10.4",
    "allure-commandline": "^2.13.0",
    "app-root-path": "^3.0.0",
    "approot": "^0.2.1",
    "chrome": "^0.1.0",
    "chromedriver": "^87.0.0",
    "driver": "^1.0.0",
    "https": "^1.0.0",
    "log4js": "^6.3.0",
    "moment": "^2.28.0",
    "oracledb": "^4.2.0",
    "pg-promise": "^10.8.1",
    "soap": "^0.33.0",
    "wdio-video-reporter": "^2.0.0"
  }
}
