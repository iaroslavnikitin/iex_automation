		if ($onDentalPage === undefined) {
			$onDentalPage = false;
		}
		_assertVisible($planSelectionBack);
		_assertVisible($planSelectionHealthInsurance);
		_assertVisible($planSelectionDentalInsurance);
		_assertVisible($planSelectionDentalInsurance);
