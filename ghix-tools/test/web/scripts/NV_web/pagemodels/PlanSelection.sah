	function clickCompareNow($plan1, $plan2, $plan3){
		clickElement(_link("Compare Now"));
		var $plans = [];
		if ($plan1) {
			$plans.push($plan1);
			if ($plan2) {
				$plans.push($plan2);
				if ($plan3) $plans.push($plan3);
			}
		}
		if(onHealthPlanComparePage()){
			verifyCompareHealthPlans($plans);
		}
		else{
			verifyCompareDentalPlans($plans);
		}
