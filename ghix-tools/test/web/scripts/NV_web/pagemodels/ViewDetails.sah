		_assertVisible(_link($plan.deductibleChild, _near(_tableHeader("Deductible (Child)"))));
	}
	_assertVisible(_link("/"+$plan.name+"/"),"Verify Plan name is displayed under plan Image");
	_assertVisible(_div($plan.tierAndType));
