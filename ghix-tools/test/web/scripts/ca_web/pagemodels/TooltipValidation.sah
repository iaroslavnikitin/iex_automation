function verifyDynamicValueTooltipsInTileOnPlanDisplay(){
	var $totalExpenceEstimateValue = "/"+"Your Total Expense Estimate compares health plans on the estimated out-of-pocket cost you pay when you get care plus the monthly premium you pay the plan. A "+'"medium use" level was used if you did not choose a use level earlier. Click "Back to preference" to change your medical services use level. Estimated Cost YearlyPremium for 12 months'+"/";
	var $qualityRatingInFuture = "Quality ratings to be reported after health plan's first two years with Covered California.";
	var $qualityRatingStarsTooltip = "/Overall Quality Rating Quality Ratings compare members' experience and medical care to national standards. The results for the quality categories below are combined to get the Overall Quality Rating: Getting the Right Care:/";
	//2 startsMembers' Care Experience:3.5 startsMembers' Plan Service Experience:2 starts";

	var $numOfPlans = _getText(_link("ps-nav__link"));
	$numOfPlans = parseInt(_extract($numOfPlans, "/(.*) Health Plans/", true));

	var $numOfPages = _getText(_span("cp-pagination__content", _in(_div("pagination"))));
	$numOfPages = parseInt(_extract($numOfPages, "/1 of (.*)/", true));
	var $averageFlag = 0;
	var $higherFlag = 0;
	var $lowerFlag = 0;
	var $qualityRatingInFutureFlag = 0;
	var $qualityRatingStarsFlag = 0;

	_assertVisible(_span("1 of "+$numOfPages, _in(_div("pagination"))),"Pagination should be started from 1");
	_assertTrue((_getAttribute(_link("/Previous/",_in(_div("pagination"))), "class").indexOf("disabled") != -1), "Icon Previous should be disabled")

		for (var $i=0; $i<$numOfPages; $i++){

			for(var $j=0; $j<12; $j++){

				if($numOfPlans == 0){
					break;
				}
		
		if(_isVisible(_link("Quality Rating in Future", _in(_div("cp-tile["+$j+"]"))))){
				if($qualityRatingInFutureFlag != 1){
					 _assertEqual("Quality Rating in Future", _getText(_link("Quality Rating in Future", _in(_div("cp-tile["+$j+"]")))));
//					verifyTooltip(_link("Quality Rating in Future", _in(_div("cp-tile["+$j+"]"))), $qualityRatingInFuture);
					$qualityRatingInFutureFlag++;
				}
			}
		else if(_isVisible(_link("quality-rating", _in(_div("cp-tile["+$j+"]"))))  ){
				   if($qualityRatingStarsFlag != 1){
					verifyTooltip(_link("quality-rating", _in(_div("cp-tile["+$j+"]"))), $qualityRatingStarsTooltip);
					$qualityRatingStarsFlag++;
				}
			}
	


			if(_isVisible(_link("Lower", _in(_div("cp-tile["+$j+"]"))))){
				if($lowerFlag != 1){
					verifyTooltip(_link("Lower", _in(_div("cp-tile["+$j+"]"))), $totalExpenceEstimateValue);
					$lowerFlag++;
				}
			}

			if(_isVisible(_link("Average", _in(_div("cp-tile["+$j+"]"))))){
				if($averageFlag != 1){
					verifyTooltip(_link("Average", _in(_div("cp-tile["+$j+"]"))), $totalExpenceEstimateValue);
					$averageFlag++;
				}
			}

			if(_isVisible(_link("Higher", _in(_div("cp-tile["+$j+"]"))))){
				if($higherFlag != 1){
					verifyTooltip(_link("Higher", _in(_div("cp-tile["+$j+"]"))), $totalExpenceEstimateValue);
					$higherFlag++;
				}
			}
			$numOfPlans--;
		}

	if($numOfPlans == 0){
		break;
	}

	if($qualityRatingStarsFlag == 1 && $qualityRatingInFutureFlag == 1 && $lowerFlag == 1 && $averageFlag == 1 && $higherFlag == 1){
		break;
	}
	_click(_link("/Next/", _in(_div("pagination"))));
	_assertVisible(_span("/"+($i+2)+" of "+$numOfPages+"/", _in(_div("pagination"))),"Pagination should change");
	_assertTrue((_getAttribute(_link("/Previous/",_in(_div("pagination"))), "class").indexOf("disabled") == -1), "Icon Previous should NOT be disabled")
		}

_assertTrue (($qualityRatingStarsFlag == 1 && $qualityRatingInFutureFlag == 1 && $lowerFlag == 1 && $averageFlag == 1 && $higherFlag == 1), "Verify that all values were checked");
_log("Values with 0 were NOT verified!!!");
_log("quality Rating Stars Flag = "+$qualityRatingStarsFlag);
_log("quality Rating In FutureFlag = "+$qualityRatingInFutureFlag);

_log("lower Flag = "+$lowerFlag);
_log("average Flag = "+$averageFlag);
_log("higher Flag = "+$higherFlag);


}

