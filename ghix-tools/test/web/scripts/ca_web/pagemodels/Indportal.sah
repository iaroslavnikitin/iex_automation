		$renewalApplicantsDentalIdArr[$i] = $renewalApplicants.dental[$i].SSAP_APPLICANT_ID;
	}
	$applicantsIdArr.sort();
	$renewalApplicantsHealthIdArr.sort();
	$renewalApplicantsDentalIdArr.sort();
