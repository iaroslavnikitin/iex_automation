	_log("$eventDate: "+$eventDate);
	_log("$enrollmentStartDate: "+$enrollmentStartDate);
	_log("$applicationType: "+$applicationType);

	//need to confirm logic for SEP application 
	if($applicationType == "QEP" || $applicationType == "OE"){
