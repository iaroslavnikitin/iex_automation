function eligibilityResultsOfMembers($memberEligibility){
	
		
	_assertVisible(_heading2("Eligibility Results for members of your household"));
	$stateSubsidyVisible = isStateSubsidyConfigEnabled();
	
	for(var $i =0; $i<$ssapPeople.length; $i++){
		var $fullName = $ssapPeople[$i].fullName(true);
		_assertVisible(_cell($fullName));
		
		
		if($memberEligibility[$i] == "Medicaid Referral" || $memberEligibility[$i] == "Not seeking coverage" || $memberEligibility[$i] == "Potentially Medicaid Eligible" ||$memberEligibility[$i] =="Potentially CHIP Eligible"){
			_assertVisible(_listItem("/"+$memberEligibility[$i]+"/", _rightOf(_cell($fullName))));
			//_assertVisible(_listItem("Not EligibleMember is not eligible to enroll in plan on the Marketplace. to enroll in a Market Place Health or Dental Plan", _rightOf(_cell($fullName))));
			//if($memberEligibility[$i] =="Potentially NJ FamilyCare eligible"){
			//	_assertVisible(_paragraph("No Action Needed. NJ FamilyCare will contact you.",_rightOf(_cell($fullName))));
				
		//	}
			//else{
			//	_assertNotVisible(_paragraph("/No Action Needed/",_rightOf(_cell($fullName))))
			//}
		}
		else if($memberEligibility[$i] == "APTC/CSR" || $memberEligibility[$i] == "APTC" || $memberEligibility[$i] == "QHP" ){
			if($memberEligibility[$i] == "APTC"){
				_assertVisible(_listItem("/Advanced Premium Tax Credit/", _rightOf(_cell($fullName))));
				if($stateSubsidyVisible =='Y'){
					_assertVisible(_link("NJ Health Plan Savings", _near(_cell($fullName))));
				}
			}
			if($memberEligibility[$i] == "APTC/CSR"){
				_assertVisible(_listItem("/Advanced Premium Tax Credit/", _rightOf(_cell($fullName))));
				if($stateSubsidyVisible =='Y'){
					_assertVisible(_link("NJ Health Plan Savings", _near(_cell($fullName))));
				}
				_assertVisible(_listItem("/Cost Sharing Reduction/", _rightOf(_cell($fullName))));
			}
			if($memberEligibility[$i] == "CSR"){
				_assertVisible(_listItem("/Cost Sharing Reduction/", _rightOf(_cell($fullName))));
			}
		_assertVisible(_listItem("Marketplace Qualified Health and Dental plans", _near(_cell($fullName))));
		}
		
	}
}

function verifyOptionalAction($otherOptionFor, $eligibility){
	_assertVisible(_heading2("Optional actions"));
	_assertVisible(_heading3("Other Options for Medicaid Assessed Individuals"));
	_click(_heading3("seeks-QHP__toggle"));
	_assertVisible(_paragraph("seeks-QHP__note"));
	_assertEqual("The initial evaluation of your application found you or someone in your household to be eligible for Medicaid and your application has been sent to NJ FamilyCare. In almost all cases, NJ FamilyCare will confirm this initial assessment and you will be enrolled in free or low cost Medicaid coverage. You do not need to take any actions now. NJ FamilyCare will be in contact with you about next steps. However, you have the option to enroll in a health plan through GetCoveredNJ at full cost if you have reason to think you will not be found eligible for Medicaid by NJ FamilyCare. If you are enrolled in Medicaid, you will be disenrolled from any GetCoveredNJ health plan you have chosen. You will not get back any premiums you have paid. Select any individual(s) that you would like to enroll in a full cost GetCoveredNJ health plan.", _getText(_paragraph("seeks-QHP__note")));
