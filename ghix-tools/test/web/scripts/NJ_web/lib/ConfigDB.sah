function verifyServerDate($date){
	var $currentDate= _call(window.moment()).format("MM/DD/YYYY");
	var $serverDate = _getValue(_hidden("serverDate", _in(_div("topbar"))));
	_log("currentDate " +$currentDate);
	_log("serverDate " +$serverDate);
	_assertEqual($currentDate,_getValue(_hidden("serverDate", _in(_div("topbar")))));
}

